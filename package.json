{
  "name": "olasearch",
  "version": "2.0.2",
  "description": "OlaSearch is a fully customisable front-end search interface that brings your information collection to life.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist lib",
    "watch": "BABEL_ENV=production babel --watch src --out-dir lib",
    "release": "cross-env NODE_ENV=production webpack --display-optimization-bailout --config webpack.release.js",
    "lint": "npm run standard && eslint src",
    "nyc": "nyc npm run test",
    "test": "BABEL_ENV=production mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "coverage": "cross-env BABEL_ENV=production babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "standard": "standard --verbose --fix | snazzy",
    "retire": "retire",
    "s3-deploy": "s3-deploy './dist/olasearch.core.min.latest.js' --cwd './dist/' --region ap-southeast-1 --bucket configs.olasearch.com/assets/js"
  },
  "engines": {
    "node": ">=6.4.0",
    "npm": ">=3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/olasearch/olasearch-core.git"
  },
  "keywords": [
    "olasearch"
  ],
  "author": "Vinay M <vinay@olasearch.com> (http://gitlab.com/olasearch)",
  "license": "Copyright 2016-2017 Ola Search Pte Ltd",
  "bugs": {
    "url": "https://gitlab.com/olasearch/olasearch-core/issues"
  },
  "homepage": "https://gitlab.com/olasearch/olasearch-core",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/test",
      "/lib",
      "/src/polyfill.js",
      "/node_modules",
      "src/components/Answer/AnswerQuick.js"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      ".*.js",
      "*setup.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-olasearch": "^1.0.4",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-register": "^6.26.0",
    "chokidar": "^1.7.0",
    "cross-env": "^5.1.1",
    "enzyme": "^3.2.0",
    "eslint": "3.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.5.0",
    "expect": "^1.20.2",
    "expose-loader": "^0.7.4",
    "express": "^4.16.2",
    "isparta": "^4.0.0",
    "jsdom": "^11.3.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "react": "^16.1.1",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.1.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "retire": "^1.4.0",
    "rimraf": "^2.6.2",
    "s3-deploy": "^0.7.6",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "webpack": "^3.8.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  },
  "peerDependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "classnames": "^2.2.5",
    "dom-scroll-into-view": "^1.2.1",
    "fecha": "^2.3.1",
    "hoist-non-react-statics": "^2.3.1",
    "ismobilejs": "^0.4.1",
    "nouislider": "^10.1.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "ramda": "^0.25.0",
    "react-list": "^0.8.8",
    "react-onclickoutside": "^6.7.0",
    "react-pikaday-datepicker": "3.0.1",
    "react-responsive": "^4.0.0",
    "redux-logger": "^3.0.6",
    "xss-filters": "^1.2.7"
  },
  "pre-commit": [
    "standard"
  ]
}
