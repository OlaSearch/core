{
  "name": "olasearch",
  "version": "2.1.5",
  "description": "OlaSearch is a fully customisable front-end search interface that brings your information collection to life.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist lib",
    "watch": "BABEL_ENV=production babel --watch src --out-dir lib",
    "watch2": "BABEL_ENV=production babel --watch ./node_modules/rambda/modules --out-dir ./node_modules/rambda/modules2",
    "release": "cross-env NODE_ENV=production webpack --config webpack.release.js --json > stats.json && gulp build",
    "lint": "npm run standard && eslint src",
    "nyc": "nyc npm run test",
    "test": "BABEL_ENV=production mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch",
    "coverage": "cross-env BABEL_ENV=production babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive",
    "retire": "retire",
    "s3:deploy": "s3-deploy './dist/olasearch.core.min.latest.js' --cwd './dist/' --region ap-southeast-1 --bucket configs.olasearch.com/assets/js --cache 604800,public && s3-deploy './dist/olasearch.core.min.css' --cwd './dist/' --region ap-southeast-1 --bucket configs.olasearch.com/assets/css --cache 604800,public",
    "cdn:invalidate": "cf-invalidate E1MMHSLEW3SZPY /assets/*",
    "styleguide": "styleguidist server",
    "styleguidist:build": "styleguidist build",
    "prettier-standard": "prettier-standard 'src/**/!(polyfill)*.js'"
  },
  "prettier": {
    "semi": false
  },
  "engines": {
    "node": ">=6.4.0",
    "npm": ">=3"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/olasearch/olasearch-core.git"
  },
  "keywords": [
    "olasearch"
  ],
  "author": "Vinay M <vinay@olasearch.com> (http://gitlab.com/olasearch)",
  "license": "Copyright 2016-2017 Ola Search Pte Ltd",
  "bugs": {
    "url": "https://gitlab.com/olasearch/olasearch-core/issues"
  },
  "homepage": "https://gitlab.com/olasearch/olasearch-core",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/test",
      "/lib",
      "/src/polyfill.js",
      "/node_modules",
      "src/components/Answer/AnswerQuick.js"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      ".*.js",
      "*setup.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-olasearch": "^1.0.4",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-register": "^6.26.0",
    "chokidar": "^1.7.0",
    "cloudfront-invalidate-cli": "^1.4.0",
    "cross-env": "^5.1.1",
    "enzyme": "^3.2.0",
    "eslint": "3.19.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.5.1",
    "expect": "^1.20.2",
    "expose-loader": "^0.7.4",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "isparta": "^4.0.0",
    "jsdom": "^11.5.1",
    "lint-staged": "^6.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "prettier-standard": "^7.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-styleguidist": "^6.0.33",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "retire": "^1.4.0",
    "rimraf": "^2.6.2",
    "s3-deploy": "^0.8.0",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^3.9.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  },
  "peerDependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "dependencies": {
    "babel-runtime": "^6.25.0",
    "classnames": "^2.2.5",
    "dom-scroll-into-view": "^1.2.1",
    "fecha": "^2.3.2",
    "hoist-non-react-statics": "^2.3.1",
    "ismobilejs": "^0.4.1",
    "nouislider": "^10.1.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "rambda": "^1.0.5",
    "react-list": "^0.8.8",
    "react-onclickoutside": "^6.7.0",
    "react-pikaday-datepicker": "3.0.1",
    "redux-logger": "^3.0.6",
    "xss-filters": "^1.2.7"
  },
  "lint-staged": [
    "prettier-standard"
  ]
}
