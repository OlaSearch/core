{
  "name": "olasearch",
  "version": "0.1.11",
  "description": "OlaSearch is a fully customisable front-end search interface that brings your information collection to life.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf dist lib",
    "build:lib": "NODE_ENV=production BABEL_ENV=production babel src --out-dir lib",
    "watch": "BABEL_ENV=production babel --watch src --out-dir lib",
    "build:umd": "NODE_ENV=production webpack src/index.js dist/olasearch.js --config webpack.config.development.js",
    "build:umd:min": "NODE_ENV=production webpack --config webpack.config.production.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd:min",
    "build:umd:demo": "NODE_ENV=production webpack --config webpack.config.demo.js",
    "demo": "npm run build:umd:demo",
    "publish": "npm run clean && npm run build:lib",
    "generateConfig": "NODE_ENV=production webpack --config webpack.config.olasearch.js",
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "BABEL_ENV=production mocha --compilers js:babel-core/register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olasearch/core"
  },
  "keywords": [
    "olasearch"
  ],
  "author": "Vinay M <vinay@olasearch.com> (http://github.com/rmdort)",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/olasearch/core/issues"
  },
  "homepage": "https://github.com/olasearch/core",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "cross-env": "^1.0.6",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.13.4",
    "expose-loader": "^0.7.1",
    "express": "^4.13.3",
    "jsdom": "^8.0.2",
    "mocha": "^2.3.4",
    "react": "^0.14.7",
    "react-dom": ">= 0.14.0 < 0.15.0",
    "react-addons-test-utils": "^0.14.6",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3",
    "rimraf": "^2.5.0",
    "webpack": "^2.0.6-beta",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "peerDependencies": {
    "react": ">= 0.14.0 < 0.15.0",
    "react-dom": ">= 0.14.0 < 0.15.0",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3"
  },
  "dependencies": {
    "intl": "^1.0.1",
    "intl-locales-supported": "^1.0.0",
    "ismobilejs": "^0.3.9",
    "jquery-param": "^0.2.0",
    "query-string": "^3.0.0",
    "ramda": "^0.19.1",
    "react-intl": "^2.0.0-beta-2",
    "react-mq": "github:rmdort/react-mq",
    "react-onclickoutside": "^4.5.0",
    "react-list": "^0.7.13",
    "classnames": "^2.2.3",
    "nouislider": "^8.2.1"
  }
}
